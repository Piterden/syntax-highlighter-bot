{
  "name": "syntaxhighlightbot",
  "version": "1.0.0",
  "description": "The bot for creating an image with highlighted code from a message.",
  "main": "index.js",
  "scripts": {
    "migrate": "npm run migrate:up",
    "migrate:up": "knex migrate:latest",
    "migrate:down": "knex migrate:rollback",
    "migrate:refresh": "knex migrate:rollback && knex migrate:latest",
    "dev": "nodemon -V index.js",
    "lint": "eslint ."
  },
  "author": "Denis Efremov <efremov.a.denis@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/Piterden/syntax-highlighter-bot.git"
  },
  "license": "MIT",
  "dependencies": {
    "dotenv": "^8.0.0",
    "esm": "^3.2.25",
    "express": "^4.17.0",
    "highlight.js": "^9.15.6",
    "hoek": "^5.0.4",
    "image-size": "^0.6.3",
    "knex": "^0.17.1",
    "objection": "^1.6.8",
    "pg": "^7.11.0",
    "sshpk": "^1.16.1",
    "telegraf": "^3.29.0",
    "webshot": "^0.18.0"
  },
  "devDependencies": {
    "@atomix/eslint-config": "^7.0.0-next.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "nodemon": "^1.19.1"
  }
}
